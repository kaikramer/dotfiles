{
    "languageserver": {
        "groovy": {
            "command": "java",
            "args": [
                "-jar",
                "/home/kk/bin/groovy-language-server-all.jar"
            ],
            "filetypes": [
                "groovy"
            ]
        }
    },
    "python.jediEnabled": false,
    "clangd.path": "/home/kk/.config/coc/extensions/coc-clangd-data/install/10.0.0/clangd_10.0.0/bin/clangd",
    "diagnostic-languageserver.trace.server": "verbose",
    "diagnostic-languageserver.filetypes": {
        "yaml.ansible": "ansible_lint"
    },
    "diagnostic-languageserver.linters": {
        "ansible_lint": {
            "command": "ansible-lint",
            "args": [
                "--parseable-severity",
                "-w",
                "%file"
            ],
            "rootPatterns": [
                "ansible",
                "playbooks",
                "tasks",
                "ansible.cfg",
                ".git"
            ],
            "sourceName": "ansible-lint",
            "formatPattern": [
                "^([^:]+):(\\d+):\\s*(\\S+\\s*)\\[(\\S+?)\\]\\s*(.*)$",
                {
                    "sourceName": 1,
                    "sourceNameFilter": true,
                    "line": 2,
                    "security": 4,
                    "message": [
                        3,
                        5
                    ]
                }
            ],
            "securities": {
                "VERY_HIGH": "error",
                "HIGH": "warning",
                "MEDIUM": "warning",
                "LOW": "info",
                "VERY_LOW": "hint",
                "INFO": "hint"
            }
        }
    }
}
